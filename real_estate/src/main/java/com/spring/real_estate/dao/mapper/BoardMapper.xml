<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.real_estate.dao.BoardDao">            
    <select id="selBoard" resultType="com.spring.real_estate.vo.BoardVO">
        SELECT *
        FROM board
        ORDER BY board_seq DESC
        LIMIT 6
    </select>
</mapper>